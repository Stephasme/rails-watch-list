<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Add a List</h1>
      <%= simple_form_for @movie do |f| %>
        <%= f.input :title %>
        <%= f.input :overview %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
      <br>
      <%= link_to "Back to Main", movies_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
